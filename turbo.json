{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["WWW_SERVER_URL", "DB_URL", "DRIZZLE_DB_URL"],
      "outputs": ["dist/**", ".next/**"]
    },
    "preview": {
      "env": ["WWW_SERVER_URL", "DB_URL", "DRIZZLE_DB_URL"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "format:check": {
      "cache": false,
      "outputs": []
    },
    "format:write": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {},
    "dev": {
      "cache": false
    },
    "check": {
      "cache": false
    },
    "test": {
      "cache": false,
      "outputs": []
    },
    "db:studio": {
      "cache": false,
      "env": ["WWW_SERVER_URL", "DB_URL", "DRIZZLE_DB_URL"]
    },
    "db:push": {
      "cache": false,
      "env": ["WWW_SERVER_URL", "DB_URL", "DRIZZLE_DB_URL"]
    },
    "db:generate": {
      "cache": false,
      "env": ["WWW_SERVER_URL", "DB_URL", "DRIZZLE_DB_URL"]
    }
  }
}
